{
  "manifest_version": 3,
  "name": "CUST Odoo Pro",
  "description": "An extension that enhances the Odoo CUST website with advanced features. Developed by Hammad Mustafa.",
  "version": "1.2",
  "permissions": [],
  "host_permissions": [
    "https://odoo.cust.edu.pk/*",
    "https://tasjeel.cust.edu.pk/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/odoo_logo_16.png",
      "48": "icons/odoo_logo_48.png",
      "128": "icons/odoo_logo_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://odoo.cust.edu.pk/*",
        "https://tasjeel.cust.edu.pk/*"
      ],
      "js": [
        "jquery-3.6.0.min.js", 
        "crypto-js.min.js", 
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/odoo_logo_16.png",
    "48": "icons/odoo_logo_48.png",
    "128": "icons/odoo_logo_128.png"
  }
}
